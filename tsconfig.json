{
	"extends": "astro/tsconfigs/base",
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"@components/*": ["src/components/*"],
			"@sections/*": ["src/sections/*"],
			"@assets/*": ["src/assets/*"],
			"@js/*": ["src/js/*"],
			"@src/*": ["src/*"],
			"@base/*": ["public/*"]
		},
		"jsx": "react-jsx",
		"jsxImportSource": "react"
	}
}
